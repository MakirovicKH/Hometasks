@model List<Teacher>

<h2>Teachers List</h2>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Surname</th>
        <th>Subject</th>
    </tr>
    @foreach (var teacher in Model)
    {
        <tr>
            <td>@teacher.Id</td>
            <td>@teacher.Name</td>
            <td>@teacher.Surname</td>
            <td>@teacher.Subject</td>
        </tr>
    }
</table>
