@model List<Student>

<h2>Students List</h2>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Surname</th>
        <th>Group</th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Id</td>
            <td>@student.Name</td>
            <td>@student.Surname</td>
            <td>@student.GroupId</td>
        </tr>
    }
</table>
